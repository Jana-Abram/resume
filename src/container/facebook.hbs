
    <main>
        <h2>{{name}}</h2>
        <h3>Users</h3>
  {{#each users}}
    <ul>
      <li>ID: {{id}}</li>
      <li>Name: {{name}}</li>
      <li>Gender: {{gender}}</li>
      <li>Age: {{age}}</li>
      {{#if friends}}
        <li>Friends:
          <ul>
            {{#each friends}}
              <li>
                ID: {{id}}
                Name: {{name}}
                Gender: {{gender}}
                Age: {{age}}
                {{#if mutual_friends}}
                  <ul>
                    {{#each mutual_friends}}
                      <li>
                        ID: {{id}}
                        Name: {{name}}
                        Gender: {{gender}}
                        Age: {{age}}
                      </li>

                    {{/each}}
                  </ul>
                {{/if}}
              </li>
            {{/each}}
          </ul>
        </li>
          {{/if}}
      {{#if groups}}
        <li>Groups:
          <ul>
            {{#each groups}}
              <li>
                ID: {{id}}
                Name: {{name}}
                Description: {{description}}
                {{#if members}}
                  <ul>
                    {{#each members}}
                      <li>
                        ID: {{id}}
                        Name: {{name}}
                        Gender: {{gender}}
                        Age: {{age}}
                      </li>
                    {{/each}}
                  </ul>
                {{/if}}
              </li>
            {{/each}}
          </ul>
        </li>
      {{/if}}
    </ul>
  {{/each}}
 </main>