<div>
<div class="container">
  {{#each web.languages}}
    <div class="text-center">
      <h2>{{name}}</h2>
      <p>Version: {{version}}</p>
      <p>Description: {{description}}</p>
      <hr>

      {{#if elements}}
        <h3>Elements</h3>
        <ul>
          {{#each elements}}
              <p><strong>Name:</strong> {{name}}</p>
              <p><strong>Description:</strong> {{description}}</p>
              {{#if attributes}}
              <hr>
              
                <h4>Attributes</h4>
                <ul>
                  {{#each attributes}}
                      <p><strong>Name:</strong> {{name}}</p>
                      <p><strong>Description:</strong> {{description}}</p>
                  {{/each}}
                </ul>
              {{/if}}
          {{/each}}
        </ul>
      {{/if}}

      {{#if properties}}
        <h3>Properties</h3>
        <ul>
          {{#each properties}}
              <p><strong>Name:</strong> {{name}}</p>
              <p><strong>Description:</strong> {{description}}</p>
              {{#if values}}
                <h4>Values</h4>
                <ul>
                  {{#each values}}
                      <p><strong>Value:</strong> {{value}}</p>
                      <p><strong>Description:</strong> {{description}}</p>
                  {{/each}}
                </ul>
              {{/if}}
          {{/each}}
        </ul>
      {{/if}}

      {{#if functions}}
        <h3>Functions</h3>
        <ul>
          {{#each functions}}
              <p><strong>Name:</strong> {{name}}</p>
              <p><strong>Description:</strong> {{description}}</p>
              {{#if parameters}}
                <h4>Parameters</h4>
                <ul>
                  {{#each parameters}}
                      <p><strong>Name:</strong> {{name}}</p>
                      <p><strong>Type:</strong> {{type}}</p>
                      <p><strong>Description:</strong> {{description}}</p>
                  {{/each}}
                </ul>
              {{/if}}
          {{/each}}
        </ul>
      {{/if}}
    </div>
  {{/each}}
</div>
</div>


