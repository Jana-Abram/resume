<div>
  {{#each web.languages}}
    <div>
      <h2>{{name}}</h2>
      <p>Version: {{version}}</p>
      <p>Description: {{description}}</p>

      {{#if elements}}
        <h3>Elements</h3>
        <ul>
          {{#each elements}}
            <li>
              <p><strong>Name:</strong> {{name}}</p>
              <p><strong>Description:</strong> {{description}}</p>
              
              {{#if attributes}}
                <h4>Attributes</h4>
                <ul>
                  {{#each attributes}}
                    <li>
                      <p><strong>Name:</strong> {{name}}</p>
                      <p><strong>Description:</strong> {{description}}</p>
                    </li>
                  {{/each}}
                </ul>
              {{/if}}
            </li>
          {{/each}}
        </ul>
      {{/if}}

      {{#if properties}}
        <h3>Properties</h3>
        <ul>
          {{#each properties}}
            <li>
              <p><strong>Name:</strong> {{name}}</p>
              <p><strong>Description:</strong> {{description}}</p>
              
              {{#if values}}
                <h4>Values</h4>
                <ul>
                  {{#each values}}
                    <li>
                      <p><strong>Value:</strong> {{value}}</p>
                      <p><strong>Description:</strong> {{description}}</p>
                    </li>
                  {{/each}}
                </ul>
              {{/if}}
            </li>
          {{/each}}
        </ul>
      {{/if}}

      {{#if functions}}
        <h3>Functions</h3>
        <ul>
          {{#each functions}}
            <li>
              <p><strong>Name:</strong> {{name}}</p>
              <p><strong>Description:</strong> {{description}}</p>
              
              {{#if parameters}}
                <h4>Parameters</h4>
                <ul>
                  {{#each parameters}}
                    <li>
                      <p><strong>Name:</strong> {{name}}</p>
                      <p><strong>Type:</strong> {{type}}</p>
                      <p><strong>Description:</strong> {{description}}</p>
                    </li>
                  {{/each}}
                </ul>
              {{/if}}
            </li>
          {{/each}}
        </ul>
      {{/if}}
    </div>
  {{/each}}
</div>